<Ui xmlns='http://www.blizzard.com/wow/ui/'>
	<!-- ElvUIChat: Static Popup Template (chat-only addon) -->
	<!-- Remove the scripts added from GameDialogMixin -->
	<Frame name='ElvUIStaticPopupTemplate' inherits='StaticPopupTemplate' toplevel='true' enableKeyboard='true' enableMouse='true' hyperlinksEnabled='true' virtual='true'>
		<Scripts>
			<OnUpdate></OnUpdate>
			<OnEvent></OnEvent>
			<OnShow></OnShow>
			<OnHide></OnHide>
			<OnLoad></OnLoad>
		</Scripts>
	</Frame>
</Ui>
